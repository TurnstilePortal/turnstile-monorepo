{
  "name": "turnstile-contracts",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/TurnstilePortal/turnstile-contracts.git"
  },
  "scripts": {
    "build": "pnpm recursive run build",
    "build-artifacts": "pnpm recursive run --filter \"@turnstile-portal/*artifacts*\" build",
    "build-packages": "pnpm recursive run --filter \"@turnstile-portal/*\" build",
    "test": "pnpm -r run test",
    "changeset": "changeset",
    "version": "changeset version && pnpm install --no-frozen-lockfile",
    "publish-packages": "changeset publish",
    "clean": "pnpm recursive run clean ; rm -rf node_modules/*",
    "repl": "tsx packages/repl/src/repl.ts",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "update-aztec-version": "tsx scripts/update-aztec-deps.ts"
  },
  "pnpm": {
    "overrides": {
      "@aztec/**": "1.2.1",
      "glob": "^11.0.0"
    },
    "onlyBuiltDependencies": [
      "bcrypto",
      "bigint-buffer",
      "c-kzg",
      "esbuild",
      "leveldown",
      "lmdb",
      "msgpackr-extract",
      "protobufjs",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "@aztec/**": "1.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@changesets/cli": "^2.29.6",
    "@types/node": "^22.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "glob": "^11.0.3",
    "node-gyp": "^11.4.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
