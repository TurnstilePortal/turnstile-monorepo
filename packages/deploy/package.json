{
  "name": "@turnstile-portal/deploy",
  "version": "0.2.0",
  "description": "Turnstile Deployment Tools",
  "type": "module",
  "bin": {
    "turnstile-deploy": "./dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./commands": {
      "import": "./dist/commands/common.js",
      "require": "./dist/commands/common.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TurnstilePortal/turnstile-contracts.git"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist node_modules",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@aztec/accounts": "0.87.2",
    "@aztec/aztec.js": "0.87.2",
    "@aztec/noir-contracts.js": "0.87.2",
    "@aztec/protocol-contracts": "0.87.2",
    "@turnstile-portal/aztec-artifacts": "workspace:^",
    "@turnstile-portal/l1-artifacts-abi": "workspace:^",
    "@turnstile-portal/l1-artifacts-bytecode": "workspace:^",
    "@turnstile-portal/l1-artifacts-dev": "workspace:^",
    "@turnstile-portal/turnstile-dev": "workspace:^",
    "@turnstile-portal/turnstile.js": "workspace:^",
    "commander": "^12.1.0",
    "viem": "^2.23.13"
  },
  "devDependencies": {
    "@types/node": "^22.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
