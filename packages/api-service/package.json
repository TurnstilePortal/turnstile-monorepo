{
  "name": "@turnstile-portal/api-service",
  "version": "0.2.0",
  "description": "API server built with TypeScript and Fastify",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "generate": "DATABASE_URL=foo GENERATE_OPENAPI=true tsx src/index.ts",
    "test": "vitest --run",
    "coverage": "vitest --run --coverage",
    "lint": "biome lint --fix",
    "format": "biome format --fix",
    "check": "biome check --fix",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@turnstile-portal/api-common": "workspace:^",
    "@turnstile-portal/aztec-artifacts": "workspace:^",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.6.0",
    "pg": "^8.16.3",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "openapi-typescript": "^7.9.1"
  },
  "author": "",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/TurnstilePortal/turnstile-monorepo"
  }
}
